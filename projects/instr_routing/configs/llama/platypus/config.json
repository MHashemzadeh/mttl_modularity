{
    "dataset": "platypus",
    "model": "meta-llama/Llama-2-13b-hf",
    "model_family": "gpt",
    "fast_dev_run": 0,
    "optimizer": "adamw", 
    "learning_rate": 3e-4, 
    "micro_batch_size": 1, 
    "load_in_8bit": 1,          
    "wandb_project": "alpaca_tuning",
    "exp_name": "platypus_dense",
    "precision": "bf16",  
    "xrouter_normal_innit": 1,
    "train_batch_size": 16,
    "merge_A_B_seperately": 0, 
    "lora_rank": 4,  
    "num_train_epochs": 1,
    "n_skills": 1,
    "share_lora_a": 0,        
    "router_selector_cluster_temp": 0.1,
    "max_input_length": 4096,  
    "eval_every": 400,
    "warmup_steps": 100,    
    "same_lora_init": 0,    
    "lora_dropout": 0.05,
    "weight_decay": 0.00,
    "router_selector":"x_router",    
    "model_modifier": "routing_lora",
    "lora_modules": ".*mlp",  
    "lora_layers": "gate_proj|down_proj|up_proj",
    "trainable_param_names": ".*lora_[ab].*|.*selector.*",    
    "param_names_added_to_sd": ".*selector.*"
}